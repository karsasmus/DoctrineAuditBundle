{
    "name": "kricha/doctrine-audit-bundle",
    "type": "symfony-bundle",
    "description": "This bundle creates audit logs for all doctrine ORM database related changes.",
    "keywords": ["doctrine", "audit", "entity", "diff", "changeset"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksii Krychevskij",
            "email": "krich.al.vl@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "doctrine/orm": "^3.2",
        "babdev/pagerfanta-bundle": "^4",
        "symfony/framework-bundle": "^6.0|^7.0",
        "twig/twig": "^3.3",
        "pagerfanta/doctrine-dbal-adapter": "^4",
        "symfony/security-bundle": "^7.1",
        "symfony/orm-pack": "^2.4",
        "pagerfanta/twig": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Kricha\\DoctrineAuditBundle\\": "src"
        }
    },
    "version": "1.0.0"
}
